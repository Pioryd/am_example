          "login": "World",
          "password": "123",
          "options": {
            "port": 3101,
            "packet_timeout": 0
          },
          "socket_io_options": {}
        },
        "backup": {
          "url": "mongodb://127.0.0.1:27017",
          "name": "am_world_schema_1",
