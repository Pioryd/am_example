          "login": "World",
          "password": "123",
          "options": {
            "port": 3103,
            "packet_timeout": 0
          },
          "socket_io_options": {}
        }
      },
      "managers_order": {
        "initialize": [
          "world_creator",
          "api_loader",
          "mam_register",
          "admin_scripts",
          "admin_server",
          "world_server",
          "backup"
        ],
        "terminate": [
          "admin_scripts",
          "admin_server",
          "world_server",
          "world_creator",
          "api_loader",
          "mam_register",
          "backup"
        ],
        "poll": [
          "admin_scripts",
          "admin_server",
          "api_loader",
          "mam_register",
          "world_server",
          "world_creator",
          "backup"
        ]
      }
