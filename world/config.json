{
  "managers_rel_name": "managers",
  "node_modules_rel_path": "node_modules",
  "close_app_delay": 5000,
  "modules": {
    "module_world": {
      "managers_map": {
        "admin_server": {
          "login": "World",
          "password": "123",
          "options": {
            "port": 3101,
            "packet_timeout": 0
          },
          "socket_io_options": {}
        },
        "backup": {
          "url": "mongodb://127.0.0.1:27017",
          "name": "world_backup",
          "interval": 3,
          "archive_limit": 3,
          "list": []
        },
        "admin_scripts": {
          "url": "mongodb://127.0.0.1:27017",
          "name": "admin_scripts",
          "scripts_rel_name": "scripts",
          "auto_run_scripts": [
            {
              "id": "net_msg",
              "args": ["module_world", "admin_server", "false"]
            },
            {
              "id": "net_msg",
              "args": ["module_world", "world_server", "false"]
            }
          ]
        },
        "editor": {
          "data_config": {
            "type": {
              "db_data": {
                "url": "mongodb://127.0.0.1:27017",
                "name": "tutorial_1"
              },
              "actions": ["data", "update"],
              "init": { "id": "", "extends": "", "data": {}, "properties": [] },
              "validate": {
                "properties": {
                  "id": {
                    "type": "string"
                  },
                  "extends": {
                    "type": "string"
                  },
                  "data": {
                    "type": "object"
                  },
                  "properties": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                },
                "required": ["id"],
                "additionalProperties": false
              }
            },
            "object": {
              "db_data": {
                "url": "mongodb://127.0.0.1:27017",
                "name": "tutorial_1"
              },
              "actions": ["data", "update"],
              "init": {
                "id": "",
                "type": "",
                "data": {},
                "properties": [],
                "area": ""
              },
              "validate": {
                "properties": {
                  "id": {
                    "type": "string"
                  },
                  "type": {
                    "type": "string"
                  },
                  "data": {
                    "type": "object"
                  },
                  "properties": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "area": {
                    "type": "string"
                  }
                },
                "required": ["id"],
                "additionalProperties": false
              }
            },
            "am_form": {
              "db_data": {
                "url": "mongodb://127.0.0.1:27017",
                "name": "tutorial_1"
              }
            },
            "am_program": {
              "db_data": {
                "url": "mongodb://127.0.0.1:27017",
                "name": "tutorial_1"
              }
            },
            "am_system": {
              "db_data": {
                "url": "mongodb://127.0.0.1:27017",
                "name": "tutorial_1"
              }
            },
            "am_script": {
              "db_data": {
                "url": "mongodb://127.0.0.1:27017",
                "name": "tutorial_1"
              }
            }
          }
        },
        "world_creator": {
          "force_create": true,
          "api_folder": "../tutorial_data/01/api",
          "events_folder": "../tutorial_data/01/events"
        },
        "mam_register": {
          "debug": false
        },
        "world_server": {
          "login": "World",
          "password": "123",
          "options": {
            "port": 3103,
            "packet_timeout": 0
          },
          "socket_io_options": {}
        }
      },
      "managers_order": {
        "initialize": [
          "editor",
          "world_creator",
          "mam_register",
          "admin_scripts",
          "admin_server",
          "world_server",
          "backup"
        ],
        "terminate": [
          "admin_scripts",
          "admin_server",
          "world_server",
          "world_creator",
          "editor",
          "mam_register",
          "backup"
        ],
        "poll": [
          "admin_scripts",
          "admin_server",
          "mam_register",
          "editor",
          "world_server",
          "world_creator",
          "backup"
        ]
      }
    }
  }
}
